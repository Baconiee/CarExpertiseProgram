        function updateTextElement(selectId, textId) {
            let selectElement = $('#' + selectId);
            let textElement = $('#' + textId);
            let selectedValue = selectElement.val();
            let updatedText = "✔ " + selectedValue;
            textElement.text(updatedText);
            updateTextColor(textElement, selectedValue);
            updateOptionColor(selectElement, selectedValue);
        }

        function updateOptionColor(selectElement, selectedValue) {
            selectElement.change(function () {
                let selectedOption = $(this).find('option:selected');
                let value = selectedOption.val();

                if (value === "Hatasız") {
                    $(this).css("background-color", "darkgreen");
                    $(this).css("color", "white");
                } else if (
                    value === "Lokal Boyalı" ||
                    value === "Boyalı" ||
                    value === "İşlemli Boyalı"
                ) {
                    $(this).css("background-color", "goldenrod");
                    $(this).css("color", "white");
                } else if (
                    value === "Sök Tak" ||
                    value === "Söktak Boyalı" ||
                    value === "Sök Tak İşlem Boya"
                ) {
                    $(this).css("background-color", "navy");
                    $(this).css("color", "white");
                } else if (value === "Değişen" ||
                    value === "Yok") {
                    $(this).css("background-color", "darkred");
                    $(this).css("color", "white");
                }
                else {
                    $(this).css("background-color", ""); // Varsayılan rengi geri yükle
                    $(this).css("color", ""); // Varsayılan rengi geri yükle
                }
            });

            // İlk yükleme sırasında da doğru renkleri ayarlayın
            selectElement.trigger('change');
        }


        function updateTextColor(textElement, selectedValue) {
            if (selectedValue === "Hatasız") {
                textElement.css("fill", "darkgreen"); // SVG text elementlerinde renk değiştirme
                textElement.css("color", "darkgreen"); // Normal text elementlerinde renk değiştirme
            } else if (
                selectedValue === "Lokal Boyalı" ||
                selectedValue === "Boyalı" ||
                selectedValue === "İşlemli Boyalı"
            ) {
                textElement.css("fill", "goldenrod"); // SVG text elementlerinde renk değiştirme
                textElement.css("color", "goldenrod"); // Normal text elementlerinde renk değiştirme
            } else if (
                selectedValue === "Sök Tak" ||
                selectedValue === "Söktak Boyalı" ||
                selectedValue === "Sök Tak İşlem Boya"
            ) {
                textElement.css("fill", "navy"); // SVG text elementlerinde renk değiştirme
                textElement.css("color", "navy"); // Normal text elementlerinde renk değiştirme
            } else if (selectedValue === "Değişen" ||
                selectedValue === "Yok") {
                textElement.css("fill", "darkred"); // SVG text elementlerinde renk değiştirme
                textElement.css("color", "darkred"); // Normal text elementlerinde renk değiştirme
            } else {
                textElement.css("fill", ""); // Varsayılan rengi geri yükle
                textElement.css("color", ""); // Varsayılan rengi geri yükle
            }
        }


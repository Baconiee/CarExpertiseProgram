@model IEnumerable<CarExpertise.Models.EkspertizSablon>


<!DOCTYPE html>
<html>
<head>
    <title>Araba Bilgileri</title>
    <!-- jQuery kütüphanesini çağırın -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- Oluşturduğunuz site.js dosyasını çağırın -->
    <script src="/js/site.js"></script>
    <link rel="stylesheet" href="~/css/style.css" />
    <style>
        .custom-navbar {
            background-color: #150869; /* Özel bir renk (örneğin, kırmızı) */
        }

    </style>

</head>
<body>
    <div class="overlay" id="overlay"></div>

    <div class="confirm-box" id="confirmBox">
        <p>Girmiş olduğunuz telefon numarası daha önce kullanılmış, bu telefon numarasına sahip kişinin bilgilerinin otomatik doldurulmasını ister misiniz?</p>
        <div class="confirm-buttons">
            <button id="confirmYes" class="btn btn-danger" onclick="confirmSubmit()">Evet</button>
            <button id="confirmNo" class="btn btn-secondary" onclick="hideConfirmation()">Hayır</button>
        </div>
    </div>




    <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
        <button type="button" class="btn btn-outline-info" onclick="scrollToCustomerInfo()">Müşteri Bilgileri</button>
    </div>

    <div class="btn-group-vertical1" role="group" aria-label="Vertical button group1">
        <button type="button" class="btn btn-outline-info1" onclick="scrollToKaportaBoya()">Araç Kaporta Boya</button>
    </div>

    <div class="btn-group-vertical2" role="group" aria-label="Vertical button group2">
        <button type="button" class="btn btn-outline-info2" onclick="scrollToMotorMekanigiKontrol()">Araç Motor Mekaniği Kontrol</button>
    </div>

    <div class="btn-group-vertical3" role="group" aria-label="Vertical button group3">
        <button type="button" class="btn btn-outline-info3" onclick="scrollToMotorPerformans()">Motor Performans</button>
    </div>

    <div class="btn-group-vertical4" role="group" aria-label="Vertical button group4">
        <button type="button" class="btn btn-outline-info4" onclick="scrollToYürüyenAksam()">Yürüyen Aksam Kontrol</button>
    </div>

    <div class="btn-group-vertical5" role="group" aria-label="Vertical button group5">
        <button type="button" class="btn btn-outline-info5" onclick="scrollToIcAksesuar()">İç ve Dış Trim - Aksesuar</button>
    </div>

    <div class="btn-group-vertical6" role="group" aria-label="Vertical button group6">
        <button type="button" class="btn btn-outline-info6" onclick="scrollToElektronikSistem()">Hava Yastığı ve Elektronik Sistemler</button>
    </div>

    <div style="text-align: center;">
        <a class="navbar-brand text-light" asp-area="" asp-controller="Customer" asp-action="Main">
            <img src="@User.FindFirst("Logo")?.Value" alt="Logo" style="height: 150px;" /> <!-- Logo görüntüleme -->
        </a>
    </div>


    <div class="mb-3"></div>


    <form id="templateForm" action="/Try" method="post">
        <div class="btn-group-vertical8" role="group" aria-label="Vertical button group8">
            <button id="saveAndPrintButton" type="button" onclick="saveAndPrint()" class="btn btn-outline-success">Raporu Kaydet ve Yazdır</button>
        </div>
        <input type="hidden" name="cardHtml" id="cardHtmlInput">
        <div class="card-body">
            <div class="card-title d-flex justify-content-between align-items-center">
                <h4 id="customerInfo"><i class="fas fa-user"></i> Müşteri Bilgileri</h4>
                <button id="demoDoldur" type="button" class="btn btn-xs btn-outline-dark" onclick="demodoldur()">
                    <i class="bx bxs-color-fill"></i> Demo Müşteri ile Doldur
                </button>
            </div>

            <hr>
            <div class="alert alert-info" role="alert">
                Müşterinin telefon numarasını girin, eski müşteri ise diğer bilgiler otomatik dolar.
            </div>
            <div class="row mb-3">
                <div class="col-md-4">
                    <input type="text" id="mb_musteriadi" name="MusteriAd" class="form-control" placeholder="Müşteri Adı" required>
                </div>
                <div class="col-md-4">
                    <input type="text" id="mb_plaka" name="AracPlaka" class="form-control" placeholder="Plaka" required>
                    <i class="fas fa-car indik"></i>
                </div>
                <div class="col-md-4">
                    <input type="tel" id="mb_telefon" name="Telefon" class="form-control" placeholder="Telefon Numarası" required>
                    <i class="fas fa-phone indik"></i>
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-md-4">
                    <input type="text" id="mb_sase" name="SaseNo" class="form-control" placeholder="Şase No" required>
                    <i class="fas fa-barcode indik"></i>
                </div>
                <div class="col-md-4">
                    <input type="email" id="mb_email" name="Email" class="form-control" placeholder="E-mail --> info@gmail.com" required>
                    <i class="fas fa-envelope indik"></i>
                </div>
                <div class="col-md-4">
                    <select name="AracMarkaId" id="marka" class="form-select" required>
                        <option value="" hidden disabled selected> Araç Markası Seçiniz </option>
                        <!-- Diğer seçenekler buraya gelecek -->
                    </select>
                    <i class="fas fa-flag indik"></i>
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-md-4">
                    <select name="AracModel" id="model" class="form-select" required>
                        <option value="" disabled selected hidden>Araç Modeli Seçiniz</option>
                        <!-- Diğer seçenekler buraya gelecek -->
                    </select>

                </div>
                <div class="col-md-4">
                    <select name="YakitTipi" class="form-select" id="mb_yakit" required>
                        <option value="" disabled selected hidden>Yakıt Tipini Seçiniz</option>
                        <option value="Benzin">Benzin</option>
                        <option value="Dizel">Dizel</option>
                        <option value="Benzin/LPG">Benzin/LPG</option>
                        <option value="Elektrik">Elektrik</option>
                    </select>
                    <i class="fas fa-gas-pump indik"></i>
                </div>
                <div class="col-md-4">
                    <input type="number" id="mb_yil" name="Yil" class="form-control" placeholder="Yıl" required>
                    <i class="fas fa-calendar-check indik"></i>
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-md-4">
                    <input type="number" id="mb_km" name="AracKm" class="form-control" placeholder="Araç Km" required>
                    <i class="fas fa-tachometer-alt indik"></i>
                </div>
                <div class="col-md-4">
                    <select name="YedekAnahtar" id="mb_anahtar" class="form-control" required>
                        <option value="" disabled selected hidden>Araç Yedek Anahtarı</option>
                        <option value="Var">Var</option>
                        <option value="Yok">Yok</option>
                        <option value="Var Ama Çalışmıyor">Var Ama Çalışmıyor</option>
                    </select>
                    <i class="fas fa-key indik"></i>
                </div>
                <div class="col-md-4">
                    <input type="text" id="mb_alinan" name="Ucret" class="form-control" placeholder="Alınan Ücret" required>
                    <i class="fas fa-credit-card indik"></i>
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-md-4">
                    <select name="OdemeSekli" id="mb_odeme" class="form-control" required>
                        <option value="" disabled selected hidden>Ödeme Şekli</option>
                        <option value="Nakit">Nakit</option>
                        <option value="Kredi Kartı">Kredi Kartı</option>
                        <option value="Havale / EFT">Havale / EFT</option>
                        <option value="Borç Mahsup">Borç Mahsup</option>
                        <option value="Hediye">Hediye</option>
                        <option value="Crypto">Crypto</option>
                    </select>
                    <i class="fas fa-credit-card indik"></i>
                </div>
            </div>
        </div>



        function updateTextElement(selectId, textId) {
            let selectElement = $('#' + selectId);
            let textElement = $('#' + textId);
            let selectedValue = selectElement.val();
            let updatedText = "✔ " + selectedValue;
            textElement.text(updatedText);
            updateTextColor(textElement, selectedValue);
            updateOptionColor(selectElement, selectedValue);
        }

        function updateOptionColor(selectElement, selectedValue) {
            selectElement.change(function () {
                let selectedOption = $(this).find('option:selected');
                let value = selectedOption.val();

                if (value === "Hatasız") {
                    $(this).css("background-color", "darkgreen");
                    $(this).css("color", "white");
                } else if (
                    value === "Lokal Boyalı" ||
                    value === "Boyalı" ||
                    value === "İşlemli Boyalı"
                ) {
                    $(this).css("background-color", "goldenrod");
                    $(this).css("color", "white");
                } else if (
                    value === "Sök Tak" ||
                    value === "Söktak Boyalı" ||
                    value === "Sök Tak İşlem Boya"
                ) {
                    $(this).css("background-color", "navy");
                    $(this).css("color", "white");
                } else if (value === "Değişen" ||
                    value === "Yok") {
                    $(this).css("background-color", "darkred");
                    $(this).css("color", "white");
                }
                else {
                    $(this).css("background-color", ""); // Varsayılan rengi geri yükle
                    $(this).css("color", ""); // Varsayılan rengi geri yükle
                }
            });

            // İlk yükleme sırasında da doğru renkleri ayarlayın
            selectElement.trigger('change');
        }



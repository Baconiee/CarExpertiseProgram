        function scrollToBottomSlowly() {
            const windowHeight = window.innerHeight; // Sayfanın yüksekliği
            const maxScrollHeight = document.body.scrollHeight - windowHeight; // Sayfanın maksimum kaydırma yüksekliği
            const scrollStep = 2; // Her adımda kaydırma miktarı (sabit)
            const scrollDelay = 10; // Her adımda bekleme süresi (milisaniye cinsinden)
            const minScrollSpeed = 1; // Minimum kaydırma hızı

            let currentScrollPosition = window.scrollY; // Mevcut kaydırma pozisyonu
            let currentScrollSpeed = scrollStep; // Başlangıç kaydırma hızı

            function doScroll() {
                if (currentScrollPosition < maxScrollHeight) { // Maksimum kaydırma yüksekliğine ulaşıldı mı?
                    currentScrollPosition += currentScrollSpeed; // Kaydırma miktarını arttır
                    window.scrollTo(0, currentScrollPosition); // Yeni kaydırma pozisyonunu ayarla
                    setTimeout(doScroll, scrollDelay); // Belirli bir süre sonra tekrar kaydır

                    // Minimum kaydırma hızını kontrol et
                    if (currentScrollSpeed > minScrollSpeed) {
                        currentScrollSpeed -= 0.005; // Hızı yavaşça azalt
                    }
                } else {
                    // Maksimum kaydırma yüksekliğine ulaşıldığında kaydırma işlemi sonlanır
                    window.scrollTo(0, maxScrollHeight);
                }
            }

            doScroll(); // İlk kaydırma işlemini başlat
        }


                            if (textId) textElement.style.fill = "goldenrod";
                            selectElement.style.backgroundColor = "goldenrod";
                        } else if (secilenDeger === "Sök Tak" || secilenDeger === "Söktak Boyalı" || secilenDeger === "Sök Tak İşlem Boya") {
                            if (textId) textElement.style.fill = "navy";
                            selectElement.style.backgroundColor = "navy";
                        } else if (secilenDeger === "Değişen" || secilenDeger === "Yok") {
                            if (textId) textElement.style.fill = "darkred";
                            selectElement.style.backgroundColor = "darkred";
                        }
                    }
                } else {
                    if (textId) {
                        textElement.textContent = "Seçilmemiş";
                        textElement.style.fill = "gray"; // Gri renk atanması
                    }
                    selectElement.style.backgroundColor = ""; // Select kutusunun rengini sıfırla
                    selectElement.style.color = ""; // Yazı rengini sıfırla
                }
            });
        }



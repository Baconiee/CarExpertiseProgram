        async function saveAndPrint() {
            const form = document.getElementById('templateForm');
            const formData = new FormData(form);
            const requiredFields = form.querySelectorAll('[required]');
            let firstInvalidField = null;

            requiredFields.forEach(field => {
                field.style.borderColor = '';  // Temizle
                if (!field.value) {
                    field.style.borderColor = 'red';  // Boş olan alanları kırmızı yap
                    if (!firstInvalidField) {
                        firstInvalidField = field;
                    }
                }
            });

            if (firstInvalidField) {
                firstInvalidField.focus();
                firstInvalidField.scrollIntoView({ behavior: 'smooth', block: 'center' });
                return;  // Form gönderimini durdur
            }

            try {
                const response = await fetch('/Customer/CombinedModel', {
                    method: 'POST',
                    body: formData
                });

                if (response.ok) {
                    const navbar = document.querySelector('.custom-navbar');
                    if (navbar) {
                        navbar.style.display = 'none';
                    }

                    const alertInfo = document.querySelector('.alert.alert-info');
                    if (alertInfo) {
                        alertInfo.style.display = 'none';
                    }

                    const paragraphs = document.querySelectorAll('p');
                    paragraphs.forEach(p => {
                        p.style.display = 'none';
                    });

                    const buttons = document.querySelectorAll('button');
                    buttons.forEach(button => {
                        if (button.classList.contains('btn-dark-green')) {
                            button.style.display = 'none';
                        }
                    });

                    // Seçili select'leri gizle
                    const selects = ['boya-select', 'hava-select', 'motor-mekanik-select', 'yuruyen-select', 'trim-select'];
                    selects.forEach(selectId => {
                        const selectElement = document.getElementById(selectId);
                        if (selectElement) {
                            selectElement.style.display = 'none';
                        }
                    });

                    window.print();

                    // Print işleminden sonra elemanları tekrar görünür yap
                    if (navbar) {
                        navbar.style.display = 'block';
                    }
                    if (alertInfo) {
                        alertInfo.style.display = 'block';
                    }
                    paragraphs.forEach(p => {
                        p.style.display = 'block';
                    });
                    buttons.forEach(button => {
                        if (button.classList.contains('btn-dark-green')) {
                            button.style.display = 'inline-block';  // Butonları tekrar görünür yap
                        }
                    });
                    selects.forEach(selectId => {
                        const selectElement = document.getElementById(selectId);
                        if (selectElement) {
                            selectElement.style.display = 'inline-block';
                        }
                    });
                } else {
                    console.error('Form gönderimi başarısız:', response.statusText);
                }
            } catch (error) {
                console.error('Form gönderimi sırasında bir hata oluştu:', error);
            }
        }



